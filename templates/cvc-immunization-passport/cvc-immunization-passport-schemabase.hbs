"conformance-spec": "{{@fhir-profile-uri}}",
"attributes": { 
    {{#each}} {{entry}}
    [
        {{#if @last}} 
        "{{entry.resourceType}}"
        {{else}}
        "{{entry.resourceType}}",
        {{/if}}
    ]
    {{/each}}
},
