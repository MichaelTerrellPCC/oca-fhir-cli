{
"@context": {
"@vocab" : "http://hl7.org/fhir/",
"@version": 1.1,
"domain": {"@type": "@vocab"},
"dpv": "http://www.w3.org/ns/dpv#",
"dpv-gdpr": "http://www.w3.org/ns/dpv-gdpr#",
"dpv-nace": "http://www.w3.org/ns/dpv-nace#"
},
"name": "{{type}}",
"type": "spec/schema_base/1.0",
